# Uncomment the next line to define a global platform for your project
# platform :ios, ’10.0’

target 'BIRTH' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  pod ’SVProgressHUD’

  pod ’RealmSwift’

  pod 'Alamofire', '~> 4.3'

  pod ‘SwiftyJSON’

  pod ’FBSDKCoreKit'

  pod 'FBSDKLoginKit'

  pod ‘FBSDKShareKit'

  pod 'FacebookCore'  
  
  pod 'FacebookLogin'  

  # Pods for BIRTH

  target 'BIRTHTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BIRTHUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
